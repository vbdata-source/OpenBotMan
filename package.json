{
  "name": "openbotman",
  "version": "2.0.0-alpha.1",
  "description": "Multi-Agent Orchestration Platform - Autonomous AI Development Teams",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "start": "pnpm --filter @openbotman/orchestrator start",
    "api": "pnpm --filter @openbotman/api-server start",
    "web": "pnpm --filter @openbotman/web-ui dev",
    "cli": "pnpm --filter @openbotman/cli start",
    "docker:build": "docker build -t openbotman .",
    "docker:run": "docker-compose up -d"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.14.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vbdata-source/OpenBotMan.git"
  },
  "author": "vb-data e.U.",
  "license": "MIT",
  "keywords": [
    "ai",
    "agents",
    "llm",
    "orchestration",
    "multi-agent",
    "claude",
    "gpt",
    "gemini",
    "autonomous",
    "development"
  ]
}
